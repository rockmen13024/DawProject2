<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Main</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>com.stuff.HelloServlet</servlet-name>
    <servlet-class>com.stuff.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.stuff.HelloServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Buna</servlet-name>
    <jsp-file>/home.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Buna</servlet-name>
    <url-pattern>/home_test</url-pattern>
  </servlet-mapping>
</web-app>
